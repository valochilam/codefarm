	"Main Function": {
		"prefix": "main",
		"body": [
			"#include <stdio.h>",
			"",
			"int main() {",
			"    $0",
			"    return 0;",
			"}"
		],
		"description": "Main function with stdio.h"
	},
	"For Loop": {
		"prefix": "for",
		"body": [
			"for (int ${1:i} = ${2:0}; ${1:i} < ${3:n}; ${1:i}++) {",
			"    $0",
			"}"
		],
		"description": "For loop"
	},
	"While Loop": {
		"prefix": "while",
		"body": [
			"while (${1:condition}) {",
			"    $0",
			"}"
		],
		"description": "While loop"
	},
	"Do While Loop": {
		"prefix": "do",
		"body": [
			"do {",
			"    $0",
			"} while (${1:condition});"
		],
		"description": "Do while loop"
	},
	"If Statement": {
		"prefix": "if",
		"body": [
			"if (${1:condition}) "
