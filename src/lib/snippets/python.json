{
  "Main Block": {
    "prefix": "main",
    "body": [
      "def main():",
      "    $0",
      "",
      "if __name__ == \"__main__\":",
      "    main()"
    ],
    "description": "Main function with entry point"
  },
  "For Loop": {
    "prefix": "for",
    "body": [
      "for ${1:i} in range(${2:n}):",
      "    $0"
    ],
    "description": "For loop with range"
  },
  "For Each": {
    "prefix": "foreach",
    "body": [
      "for ${1:item} in ${2:items}:",
      "    $0"
    ],
    "description": "For each loop"
  },
  "While Loop": {
    "prefix": "while",
    "body": [
      "while ${1:condition}:",
      "    $0"
    ],
    "description": "While loop"
  },
  "If Statement": {
    "prefix": "if",
    "body": [
      "if ${1:condition}:",
      "    $0"
    ],
    "description": "If statement"
  },
  "If Else": {
    "prefix": "ifelse",
    "body": [
      "if ${1:condition}:",
      "    $2",
      "else:",
      "    $0"
    ],
    "description": "If-else statement"
  },
  "If Elif Else": {
    "prefix": "ifelif",
    "body": [
      "if ${1:condition}:",
      "    $2",
      "elif ${3:condition}:",
      "    $4",
      "else:",
      "    $0"
    ],
    "description": "If-elif-else statement"
  },
  "Function": {
    "prefix": "def",
    "body": [
      "def ${1:function_name}(${2:params}):",
      "    \"\"\"${3:Docstring}\"\"\"",
      "    $0"
    ],
    "description": "Function definition"
  },
  "Class": {
    "prefix": "class",
    "body": [
      "class ${1:ClassName}:",
      "    def __init__(self${2:, params}):",
      "        $0"
    ],
    "description": "Class definition"
  },
  "Try Except": {
    "prefix": "try",
    "body": [
      "try:",
      "    $1",
      "except ${2:Exception} as ${3:e}:",
      "    $0"
    ],
    "description": "Try-except block"
  },
  "List Comprehension": {
    "prefix": "listcomp",
    "body": [
      "[${1:expr} for ${2:item} in ${3:iterable}]"
    ],
    "description": "List comprehension"
  },
  "Dictionary Comprehension": {
    "prefix": "dictcomp",
    "body": [
      "{${1:key}: ${2:value} for ${3:item} in ${4:iterable}}"
    ],
    "description": "Dictionary comprehension"
  },
  "Lambda": {
    "prefix": "lambda",
    "body": [
      "lambda ${1:x}: ${0:expression}"
    ],
    "description": "Lambda function"
  },
  "Print": {
    "prefix": "print",
    "body": [
      "print(${1:message})"
    ],
    "description": "Print statement"
  },
  "Print F-String": {
    "prefix": "printf",
    "body": [
      "print(f\"${1:message}\")"
    ],
    "description": "Print with f-string"
  },
  "Input": {
    "prefix": "input",
    "body": [
      "${1:variable} = input(\"${2:prompt}\")"
    ],
    "description": "Input statement"
  },
  "Input Integer": {
    "prefix": "inputint",
    "body": [
      "${1:n} = int(input())"
    ],
    "description": "Input integer"
  },
  "Input List": {
    "prefix": "inputlist",
    "body": [
      "${1:arr} = list(map(int, input().split()))"
    ],
    "description": "Input list of integers"
  },
  "Read File": {
    "prefix": "readfile",
    "body": [
      "with open(\"${1:filename}\", \"r\") as ${2:f}:",
      "    ${3:content} = ${2:f}.read()",
      "    $0"
    ],
    "description": "Read file"
  },
  "Write File": {
    "prefix": "writefile",
    "body": [
      "with open(\"${1:filename}\", \"w\") as ${2:f}:",
      "    ${2:f}.write(${3:content})",
      "    $0"
    ],
    "description": "Write file"
  },
  "Sort": {
    "prefix": "sort",
    "body": [
      "${1:arr}.sort()"
    ],
    "description": "Sort in place"
  },
  "Sorted": {
    "prefix": "sorted",
    "body": [
      "sorted(${1:arr})"
    ],
    "description": "Return sorted list"
  },
  "Binary Search": {
    "prefix": "bisect",
    "body": [
      "from bisect import bisect_left, bisect_right",
      "",
      "def binary_search(arr, x):",
      "    i = bisect_left(arr, x)",
      "    if i != len(arr) and arr[i] == x:",
      "        return i",
      "    return -1"
    ],
    "description": "Binary search using bisect"
  },
  "GCD": {
    "prefix": "gcd",
    "body": [
      "from math import gcd"
    ],
    "description": "Import GCD"
  },
  "Defaultdict": {
    "prefix": "defaultdict",
    "body": [
      "from collections import defaultdict",
      "${1:d} = defaultdict(${2:int})"
    ],
    "description": "Defaultdict import and init"
  },
  "Counter": {
    "prefix": "counter",
    "body": [
      "from collections import Counter",
      "${1:count} = Counter(${2:iterable})"
    ],
    "description": "Counter import and init"
  },
  "Deque": {
    "prefix": "deque",
    "body": [
      "from collections import deque",
      "${1:q} = deque()"
    ],
    "description": "Deque import and init"
  },
  "Heapq": {
    "prefix": "heapq",
    "body": [
      "import heapq",
      "${1:heap} = []",
      "heapq.heappush(${1:heap}, ${2:item})"
    ],
    "description": "Heapq import and usage"
  }
}
