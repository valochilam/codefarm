  "Binary Search": {
    "prefix": "bsearch",
    "body": [
      "int ${1:l} = 0, ${2:r} = ${3:n} - 1;",
      "while (${1:l} <= ${2:r}) {",
      "    int mid = ${1:l} + (${2:r} - ${1:l}) / 2;",
      "    if (${4:arr}[mid] == ${5:target}) {",
      "        $0",
      "    } else if (${4:arr}[mid] < ${5:target}) {",
      "        ${1:l} = mid + 1;",
      "    } else {",
      "        ${2:r} = mid - 1;",
      "    }",
      "}"
    ],
    "description": "Binary search"
  },
