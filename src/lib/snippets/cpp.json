{
  "Competitive Template": {
    "prefix": "cp",
    "body": [
      "#include <bits/stdc++.h>",
      "using namespace std;",
      "",
      "#define ll long long",
      "#define pb push_back",
      "#define mp make_pair",
      "#define vi vector<int>",
      "#define vll vector<ll>",
      "#define pii pair<int, int>",
      "#define all(x) (x).begin(), (x).end()",
      "",
      "int main() {",
      "    ios_base::sync_with_stdio(false);",
      "    cin.tie(NULL);",
      "    ",
      "    $0",
      "    ",
      "    return 0;",
      "}"
    ],
    "description": "Competitive programming template"
  },
